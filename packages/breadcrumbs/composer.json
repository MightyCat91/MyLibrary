{
  "name": "mylibrary/breadcrumbs",
  "description": "Breadcrumbs for Laravel 5.",
  "keywords": ["laravel", "framework", "Laravel-Breadcrumbs", "Laravel Breadcrumbs", "mylibrary"],
  "license": "MIT",
  "authors": [{
    "name": "Alexander Muzhilkin",
    "email": "amuzhilkin@gmail.com"
  }],
  "require": {
    "php": "^7.1"
  },
  "require-dev": {
    "graham-campbell/testbench": "^3.2",
    "mockery/mockery": "^0.9.5",
    "phpunit/phpunit": "^5.6"
  },
  "autoload": {
    "psr-4": {
      "MyLibrary\\Breadcrumbs\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "MyLibrary\\Tests\\Breadcrumbs\\": "tests"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}