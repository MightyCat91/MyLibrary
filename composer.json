{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "~1.0",
        "graham-campbell/exceptions": "^10.0.0",
        "barryvdh/laravel-ide-helper": "^2.3@dev",
        "doctrine/dbal": "^2.5",
        "watson/active": "dev-master",
        "barryvdh/laravel-debugbar": "^2.3",
        "doctrine/inflector": "^1.1",
        "mylibrary/search": "1.0.0"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "^7.0"
    },
    "repositories": [
        {
            "type": "path",
            "url": "packages/search/"
        }
    ],
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "MyLibrary\\Breadcrumbs\\": "packages/breadcrumbs/src",
            "MyLibrary\\customServiceProviders\\": "app/Providers/Custom",
            "MyLibrary\\Search\\": "packages/search/src"
        },
        "files": [
            "app/Http/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}